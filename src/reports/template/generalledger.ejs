<!DOCTYPE html >
<html lang ="en" style="zoom:0.6">

    <head>
        <meta charset="utf-8">
        <!-- <meta name="viewport" content="width=1024px"> -->
        <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge"> -->
        <title>Quotation TFES </title>
           <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
       <!-- <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"> -->
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
        <!-- <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;700&display=swap" rel="stylesheet"  type='text/css>   -->
    </head>
    
<style>
    
    body {
        background: white;
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        font-size: 12px;
        -webkit-print-color-adjust: exact;
    };
    @media print {
        body {
          background: white;
        }
        .page {
          margin: 0;
          height: 100%;
          width: 100%;
        }
      }
    h1 {
        font-size:1.5rem;
        margin: 0;
    }
    h2 {
        font-size: 1.2rem;
        margin: 0;
    }
    h3 {
        font-size: 1rem;
        margin: 0;
    }
    h4 {
        font-size: 12px;
        margin: 0;
    }
    p {
        font-size: 1rem;
        margin: 0;
    }
    td, th {
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        padding: 4px 5px;
        font-size: 0.8rem;
        max-width: 150px;
        word-wrap:inherit;
    }
    td{
        font-size: 0.65rem;
    }
    .header-section-spacing {
        padding-right: 1rem;
    }
    hr { margin: 0.1rem auto; }
    .spacing-40 {
        padding : 40px 0;
    }
    .spacing-10 {
        padding : 10px 0;
    }
    .spacing-30 {
        padding : 30px 0;
    }
    .spacing-20 {
        padding : 20px 0;
    }
    .spacing-5 {
        padding : 5px 0;
    }
    .spacing-2 {
        padding : 2px 0;
    }
    .tnc {
        display: inline-block;
        width: 15%
    }
    .font-weight-bold{
        font-weight: bold;
    }
    .side-spacing-8 {
        padding: 0px 8px;
    }
    .text-center{
        text-align: center; 
        margin: 0 auto;
    }
    .text-left{
        text-align: left; 
    }
    .text-right{
        text-align: right; 
        
    }
    .debug-border {
        border: 1px solid red;
    }
    .table-border {
        border: 1px solid black;
        border-collapse: collapse;
    }
    .table-border-collapse {
        border-collapse: collapse;
    }
    .border-left{
        border-left:1px solid black;
    }
    .border-right{
        border-right:1px solid black;
    }
    .border-bottom{
        border-bottom:2px solid black;
    }
    .border-top{
        border-top:1px solid black;
    }

    /* Avoid page break in the middle of a row */
    .avoid {
        page-break-inside: avoid !important;
        margin: 4px 0px 4px;
        padding: 2px 0px 2px;
    }
  
</style>

<body>
    <!-- <page size="A4"> -->
        <!-- LOGO AND HSECTION  -->

        <div class="">
            <img width="10%" src="data:image/png;base64," alt="" />
        </div>
        <div class="text-right" 
        style="position: absolute;top:20px;right: 0;">
            <p class="" style="color: #444;">TF ENERGY SERVICES PTE LTD</p> 
        </div>
        <div  style="padding: 0;margin: 0;">
            <h1 class="spacing-10 text-center">TFES General Ledger</h1> 
            <table width="100%">
                <tr>
                    <td><h2>Start Date:</h2></td>
                    <td><p><%= startDate%></p></td>
                    <td width="50%" ></td>
                    <td><h2>End Date:</h2></td>
                    <td><p><%= endDate%></p></td>

                </tr>
            </table>   
            <hr style= "border-top: 1px solid;">
        </div>

        

        <!-- INFORMATION SECTION  -->
       <% if(data && data.length>0){%>
        <% for(var j = 0; j < data.length; j ++) {%>
        <% 
            var totalDebit =0;
            var totalCredit =0;
            var totalAmtCurrency =0;
           for(var i=0;i< data[j].journalItem.length;i++){ 
                totalDebit += data[j].journalItem[i].debit;
                totalCredit += data[j].journalItem[i].credit;
                totalAmtCurrency += data[j].journalItem[i].amountCurrency;
            } %> 
        <div class="spacing-10"></div>
        <div>
            <h3 class="spacing-5" style="padding-left: 4px;"><%= data[j].account_name%></h3>
            <table width="100%" class="table-border-collapse">
                <thead style="display: table-header-group;">
                   
                <tr class="border-bottom"  style="page-break-inside: avoid;" >
                    <th class="table-border" width="10%">Date</th>
                    <th class="table-border " width="30%">Partner</th>
                    <th class="table-border" width="11%">Ref</th>
                    <th class="table-border text-center" width="12%">Name</th>
                    <th class="table-border" width="10%">Debit</th>
                    <th class="table-border" width="10%">Credit</th>
                    <th class="table-border" width="10%">Amt Currency</th>
                    <th class="table-border" width="7%">Currency</th>
                </tr> 
                </thead>
                <tbody>
                    <% for(var i = 0; i < data[j].journalItem.length; i ++) {%>
                    <tr class="text-right"  style="page-break-inside: avoid;" >
                        <td class="table-border text-center"> 
                            <div class="avoid"><%= data[j].journalItem[i].entryDate%></</div>
                        </td>
                        <td class="table-border text-center ">
                            <div class="avoid"><%= data[j].journalItem[i].partner%></div>
                        </td>
                        <td class="table-border text-center">
                            <div class="avoid"><%= data[j].journalItem[i].reference %></div>
                        </td>
                        <td class="table-border text-center">
                            <div class="avoid"><%= data[j].journalItem[i].name %></div>
                        </td>
        
                        <td class="table-border">
                            <div class="avoid"><%=`${data[j].journalItem[i].debit.toLocaleString("en-SG", {style: "decimal", minimumFractionDigits: 2, maximumFractionDigits: 2}) }`  %></div>
                        </td>
                        <td class="table-border">
                            <% if ( typeof data[j].journalItem[i].credit == "number" ) {%>
                                <div class="avoid"><%=`${data[j].journalItem[i].credit.toLocaleString("en-SG", {style: "decimal", minimumFractionDigits: 2, maximumFractionDigits: 2})}`%></div>
                            <%}else{%>
                                <div class="avoid"><%= data[j].journalItem[i].credit %></div>
                            <%}%>     
                        </td>
                        <td class="table-border">
                            <div class="avoid"><%=`${data[j].journalItem[i].amountCurrency.toLocaleString("en-SG", {style: "decimal", minimumFractionDigits: 2, maximumFractionDigits: 2}) }`  %></div>
                        </td>
        
                        <td class="table-border">
                            <div class="avoid"><%=`${data[j].journalItem[i].currency}`  %></div>
                        </td>
                    </tr>
                    <% } %>
                    <tr class="text-right border-bottom" style= "background-color:#cecece !important"  >
                        <td class="border-left"> </td>
                        <td class=" "></td>
                        <td ></td>
                        <td class="font-weight-bold ">Total:</td>
                        <td class="table-border "><%= `${totalDebit.toLocaleString("en-SG", {style: "decimal", minimumFractionDigits: 2, maximumFractionDigits: 2})  }` %></td>
                        <td class="table-border "><%= `${totalCredit.toLocaleString("en-SG", {style: "decimal", minimumFractionDigits: 2, maximumFractionDigits: 2})  }` %></td>
                        <td class="table-border "></td>
                        <td class="border-right "></td>
                    </tr>
                </tbody>  
            </table> 
        </div> 
        <% } %>
       <% } %>
        <div id="pageFooter"></div>
        <!-- SECTION computer generated signature   -->
       
    <!-- </page> -->
    <!-- <br><br> -->
</body>
<script src="" async defer></script>
</html>