<!DOCTYPE html >
<html lang ="en" style="zoom:0.6">

    <head>
        <meta charset="utf-8">
        <!-- <meta name="viewport" content="width=1024px"> -->
        <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge"> -->
        <title>Quotation TFES </title>
           <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
       <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
        <!-- <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;700&display=swap" rel="stylesheet"  type='text/css>   -->
    </head>
    
<style>
    
    body {
        background: white;
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        font-size: 12px;
        -webkit-print-color-adjust: exact;
    };

    @media print {
        body {
          background: white;
        }

        .page {
          margin: 0;
          height: 100%;
          width: 100%;
        }
      }
   

    h4 {
        font-size: 12px;
        margin: 0;
        line-height: 15px;
    }

    p {
        font-size: 12px;
        margin: 0;
    }

    

    page {
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        background: white;
        display: block;
        margin: 0;
        margin-bottom: 0cm;
        box-shadow: 0 0 0.5cm rgba(0, 0, 0, 0.5);
    }



    td, th {
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        padding: 4px 0;
        font-size: 12px;
    }

    .header-section, terms-n-conditions-section {
        padding: 1rem;
    }

    .header-section-spacing {
        padding-right: 1rem;
    }

    .company-details p {
        font-size: 6px;
    }
    .company-details {
        margin-top: 1rem;
    }

    hr { margin: 0.1rem auto; }


    .debug-border {
        border: 1px solid red
    }

    .spacing-40 {
        padding : 40px 0;
    }

    .spacing-10 {
        padding : 10px 0;
    }

    .spacing-20 {
        padding : 20px 0;
    }

    .spacing-5 {
        padding : 5px 0;
    }
    .spacing-2 {
        padding : 2px 0;
    }
    p.company-title {
      font-weight: bold;
      font-size: 6px;
    }

    .tnc {
        display: inline-block;
        width: 15%
    }

    .font-weight-bold{
        font-weight: bold;
    }

    .side-spacing-8 {
        padding: 0px 8px;
    }
    .td-spacing {
        padding: 5px 8px;
    }
    
    .font-weight-bold{
      font-weight: bold;
    }
   
    .container-flex{
        padding :20px ; 
    }
    .float-left{
        float: left;
    }

    .float-right{
        float: right;
    }

    .signed-line{
        margin-top: 50px;
        color: black;
        border-top:1px solid black;
    }

    /* Avoid page break in the middle of a row */
    .avoid {
        page-break-inside: avoid !important;
        margin: 4px 0px 4px;
        padding: 2px 0px 2px;
    }

</style>
<body>
    <!-- <page size="A4"> -->
        <!-- LOGO AND ACCOLATES SECTION  -->
        <div class="container-fluid">
            <div class="row ">
                <div class="col-xs-3 header-section-spacing justify-content-center align-self-center">
                    <img src="tfs.png" width="90%" />
                </div>
                <div class="col-xs-3 header-section-spacing company-details" style="padding-top:10px">
                    <p class="company-title ">TF ENERGY SERVICES PTE LTD</p>
                    <p>5 TUAS AVE 10, Singapore 639129</p>
                    <p>TEL: (65) 68737133  FAX: (65) 68737833</p>
                    <p> EMAIL: sales@tfes.com.sg</a></p>
                    <p> WEB: www.tfes.com.sg</a></p>
                    <p>CO / GST Reg No :201713752D</p>
                </div>
                <div class="col-xs-6 header-section-spacing justify-content-center align-self-center" style="padding-top: 1rem">
                    <img src="bizSafeStar.jpeg" width="24%" />
                    <img src="ISO 9001-2015.jpg" width="24%" />
                    <img src="ISO 14001-2015.jpg" width="24%" />
                    <img src="ISO 45001-2018.jpg" width="24%" />
                </div>
            </div>
        </div>
        <!-- CLIENT AND PIC INFORMATION SECTION  -->
        <div class="container-fluid">
            <div class="spacing-10"></div>
            <h2 class="text-center" style="text-align: center; margin: 0 auto">DELIVERY ORDER</h1>
            <div class="spacing-10"></div>
            <div class="row spacing-5">
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Date:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4 ><%= data.deliveryDate %></h4>
                        </div>
                    </div>
                </div>
                <div class="col-xs-5">
                    <div class="row">
                        <div class="col-xs-4">
                            <h4 style="font-weight: bold">DO No:</h4>
                        </div>
                        <div class="col-xs-8">
                            <h4 ><%= data.deliveryNumber %></h4>
                        </div>
                    </div>
                </div>    
            </div>
            <div class="row">
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Order Ref:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4 ><%= data.soNumber %></h4>
                        </div>
                    </div>
                </div>
                <div class="col-xs-5">
                    <div class="row">
                        <div class="col-xs-4">
                            <h4 style="font-weight: bold">CRS :</h4>
                        </div>
                        <div class="col-xs-8">
                            <h4 ><%= data.salesPicFirstname %> <%= data.salesPicLastName %></h4>
                        </div>
                    </div>
                </div>    
            </div>
            <hr style= "border-top: 1px solid;">

            <div class="row spacing-5">
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-xs-12">
                            <h4 style="font-weight: bold">CUSTOMER</h4>
                        </div>
                    </div>
                </div>

                <div class="col-xs-5">
                    <div class="row">
                        <div class="col-xs-12">
                            <h4 style="font-weight: bold">DELIVERY TO</h4>
                        </div>   
                    </div>
                </div>
            </div>
            <hr style= "border-top: 1px solid;">

            <div class="row spacing-5">
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Company:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4><%= data.custName %></h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Address:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4 ><%= data.custAddress %></h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">PIC:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4><%= data.custPic %></h4>
                        </div> 
                    </div>
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Tel/Fax:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4><%= data.custTel %> <%=  %> </h4>
                        </div>  
                    </div>   
                </div>

                <div class="col-xs-5">
                    <div class="row">
                        <div class="col-xs-12">
                            <h4 ><%- data.soDelRemark ? data.soDelRemark.replace(/\n/g, '<br>'): null %></h4>
                        </div>
                        
                    </div>
                </div>
            </div>
            <hr style= "border-top: 1px solid;">
            <div class="row spacing-5">
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Customer PO. Number:</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4 ><%= data.custPoNum %></h4>
                        </div>
                    </div>
                </div>

                <div class="col-xs-5">
                    <div class="row">
                        <div class="col-xs-4">
                            
                        </div>
                        <div class="col-xs-8">
                           
                        </div>  
                    </div>
                </div>    
            </div>
            <hr style= "border-top: 1px solid;">
        </div>
        <div class="spacing-10"></div>
        <!-- QUOTATION TABLE SECTION  -->
        <div class="container-fluid ">
            <div class="row">
                <div class="col-xs-12">
                <table width="100%" >
                    <tr style= "border-top: 1px solid; border-bottom: 1px solid; background-color:#cecece !important">
                        <th width="8%" style="text-align: center;">S/N</th>
                        <th width="14%">Ref No </th>
                        <th width="60%">Descriptions </th>
                        <th width="10%">UOM</th>
                        <th width="8">QTY</th>
                    </tr>
                    </tr>
                    <% for(var i = 0; i < data.deliveryLines.length; i ++) {%>
                        <tr style="border-bottom: 1px solid">
                            <td style="text-align: center;" class="td-spacing"> 
                                <div class="avoid"><%= i+1 %> </div>
                            </td>
                            <td class="td-spacing"> 
                                <div class="avoid"><%= data.deliveryLines[i].custRef %> </div>
                            </td>
                            <td class="td-spacing"> 
                                <div class="avoid"><%= data.deliveryLines[i].description %> </div>
                            </td>
                            <td class="td-spacing">  
                                <div class="avoid"><%= data.deliveryLines[i].uom %></div>
                            </td>  
                            <td class="td-spacing"> 
                                <div class="avoid"><%= data.deliveryLines[i].qty %></div>
                            </td>  
                        </tr>
                    <% } %>
                </table>
                </div>
            </div>
        </div>  
        <div class="spacing-20"></div>
        <!-- SECTION computer generated signature   -->
        <div class="container-flex" >
                <div class="terms-n-conditions-section  float-left">
                    <p class="font-weight-bold">GOODS ISSUED BY</p>
                    <div class="signed-line"></div>
                    <P class="font-small">Signed and Company Stamp</P>
                </div>
                <div class="terms-n-conditions-section  float-right">
                    <p class="font-weight-bold">GOODS RECEIVED BY</p>
                    <div class="signed-line"></div>
                    <P class="font-small">Signed and Company Stamp</P>
                </div>
        </div>

         
    <!-- </page> -->
    <!-- <br><br> -->
</body>

<script src="" async defer></script>

</html>