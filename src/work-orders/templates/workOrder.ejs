<!DOCTYPE html >
<html lang ="en" style="zoom:0.6">

    <head>
        <meta charset="utf-8">
        <!-- <meta name="viewport" content="width=1024px"> -->
        <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge"> -->
        <title>Quotation TFES </title>
           <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
       <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
        <!-- <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;700&display=swap" rel="stylesheet"  type='text/css>   -->
    </head>
    
<style>
    
    body {
        background: white;
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        font-size: 12px;
        -webkit-print-color-adjust: exact;
    };

    @media print {
        body {
          background: white;
        }

        .page {
          margin: 0;
          height: 100%;
          width: 100%;
        }
      }
   

    h4 {
        font-size: 14px;
        margin: 0;
        line-height: 15px;
    }

    p {
        font-size: 12px;
        margin: 0;
    }

    page {
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        background: white;
        display: block;
        margin: 0;
        margin-bottom: 0cm;
        box-shadow: 0 0 0.5cm rgba(0, 0, 0, 0.5);
    }

    td, th {
        font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif;
        padding: 4px 0;
        font-size: 14px;
        border-top: 1px solid #000000;
    }

    .header-section, terms-n-conditions-section {
        padding: 1rem;
    }

    .header-section-spacing {
        padding-right: 1rem;
    }

    .company-details p {
        font-size: 8px;
    }
    .company-details {
        margin-top: 1rem;
    }

    hr { margin: 0.1rem auto; }


    .debug-border {
        border: 1px solid red
    }

    .spacing-40 {
        padding : 40px 0;
    }

    .spacing-10 {
        padding : 10px 0;
    }

    .spacing-20 {
        padding : 20px 0;
    }

    .spacing-5 {
        padding : 5px 0;
    }
    .spacing-2 {
        padding : 2px 0;
    }

    .side-spacing-8 {
        padding: 0px 8px;
    }

    p.company-title {
      font-weight: bold;
      font-size: 8px;
    }

    .font-weight-bold{
      font-weight: bold;
    }

    .td-spacing {
        padding: 5px 8px;
    }

    
</style>

<body>
    <!-- <page size="A4"> -->
        <!-- LOGO AND ACCOLATES SECTION  -->
        <div class="container-fluid">
            <div class="row ">
                <div class="col-xs-3 header-section-spacing justify-content-center align-self-center">
                    <img src="tfs.png" width="90%" />
                </div>
                <div class="col-xs-3 header-section-spacing company-details" style="padding-top:10px">
                    <p class="company-title ">TF ENERGY SERVICES PTE LTD</p>
                    <p>5 TUAS AVE 10, Singapore 639129</p>
                    <p>TEL: (65) 68737133  FAX: (65) 68737833</p>
                    <p> EMAIL: sales@tfes.com.sg</a></p>
                    <p> WEB: www.tfes.com.sg</a></p>
                    <p>CO / GST Reg No :201713752D</p>
                </div>
                <div class="col-xs-6 header-section-spacing justify-content-center align-self-center" style="padding-top: 1rem">
                    <img src="bizSafeStar.jpeg" width="24%" />
                    <img src="ISO 9001-2015.jpg" width="24%" />
                    <img src="ISO 14001-2015.jpg" width="24%" />
                    <img src="ISO 45001-2018.jpg" width="24%" />
                </div>
            </div>
        </div>

        <!-- CLIENT AND PIC INFORMATION SECTION  -->
        <div class="container-fluid">
            <div class="spacing-10"></div>
            <h2 class="text-center" style="text-align: center; margin: 0 auto">WORK ORDER</h1>
            <div class="spacing-10"></div>

            <div class="row spacing-5">
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">WO No :</h4>
                        </div>
                        <div class="col-xs-9">
                            <h4 > <%= data.woNumber %></h4>
                        </div>
                    </div>
                </div>
                <div class="col-xs-5">
                    <div class="row">
                        <div class="col-xs-5">
                            <h4 style="font-weight: bold">Order Ref:</h4>
                        </div>
                        <div class="col-xs-7">
                            <h4 ><%= data.soNumber %></h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-5">
                            <h4 style="font-weight: bold">Sales PIC:</h4>
                        </div>
                        <div class="col-xs-7">
                            <h4 ><%= data.salesPIC %></h4>
                        </div>
                    </div>
                </div>
                
            </div>
            <hr style= "border-top: 1px solid;">
            <div class="row spacing-5">
                <div class="col-xs-7">
                    <div class="row">
                        
                        <div class="col-xs-3">
                            <h4 style="font-weight: bold">Remark:</h4>
                        </div>
                        <div class="col-xs-9">
                            <% if(data.remark != undefined && data.remark != "") { %>
                            <h4> <%- data.remark.replace(/\n/g, '<br>') %> </h4>
                            <% } %>
                        </div> 
                    </div>
                </div>

                <div class="col-xs-5">
                    <div class="row">  
                        <div class="col-xs-5">
                            <h4 style="font-weight: bold">Date Created :</h4>
                        </div>
                        <div class="col-xs-7">
                            <h4><%= data.createAt %></h4>
                        </div>
                    </div>
                    <div class="row">  
                        <div class="col-xs-5">
                            <h4 style="font-weight: bold">Date Completed :</h4>
                        </div>
                        <div class="col-xs-7">
                            <h4><%= data.completedDate %></h4>
                        </div>
                    </div>
                </div>
            </div>
            <hr style= "border-top: 1px solid;">
        </div>
        <div class="spacing-10"></div>
        <!-- QUOTATION TABLE SECTION  -->
        <div class="container-fluid ">
            <div class="row">
                <div class="col-xs-12">
                  <table width="100%" >
                    <tr style= "border-top: 1px solid; border-bottom: 1px solid; background-color:#cecece !important">
                        <th width="8%" class="side-spacing-8">No.</th>
                        <th width="10%" class="side-spacing-8">Type</th>
                        <th width="40%" class="side-spacing-8">Descriptions </th>
                        <th width="20%" class="side-spacing-8">Picked Sku </th>
                        <th width="14%" class="side-spacing-8">Location </th>
                        <th width="8%" class="side-spacing-8">qty</th>
                    </tr>
                  
                    <%  for (const doItem of data.workOrderItems) {%>


                        <% if (doItem.workType === "Pick/Pack") { %>

                            <% if (doItem.woPickingList.length == 1) { %>
                                <tr style="border-bottom: 1px solid" >
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= doItem.runningNum %>  </div>
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= doItem.workType %> </div>
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid">(<%= doItem.woPickingList[0].partNumber %>) <br/> <%- doItem.description.replace(/\n/g, '<br>') %> </div>
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= doItem.woPickingList[0].selectedSkuPartNumber ? (doItem.woPickingList[0].selectedSkuPartNumber) : "" %> </div>
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= doItem.woPickingList[0].selectedSkuLocation  ? doItem.woPickingList[0].selectedSkuLocation : "" %> </div>
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= doItem.qty %> </div>
                                    </td>
                                </tr>
                            <% } else { %>

                            <% for (const pickItem of doItem.woPickingList) {%>
                                <tr style="border-bottom: 1px solid">
                                    <td class="td-spacing">
                                        <div class="avoid"><%=pickItem.runningNum%></div>
                                    </td>
                                    <td class="td-spacing">
                                        <div class="avoid"><%=doItem.workType%></div>
                                    </td>
                                    <td class="td-spacing">
                                        <div class="avoid">(<%= pickItem.partNumber %>) <br/> <%- pickItem.productDesc.replace(/\n/g, '<br>') %> </div>
                                    </td>
                                    <td class="td-spacing">
                                        <div class="avoid"><%= pickItem.selectedSkuPartNumber %></div>
                                    </td>
                                    <td class="td-spacing">
                                        <div class="avoid"><%= pickItem.selectedSkuLocation %> </div>
                                    </td>
                                    <td class="td-spacing">
                                        <div class="avoid"><%=pickItem.workQty%></div>
                                    </td>
                                </tr>
                            <% } %>

                            <% } %>


                        <% } %>
                            
                        <% if (doItem.workType === "Assembly") { %>

                            <tr style="border-bottom: 1px solid" >
                                <td class="td-spacing"> 
                                    <div class="avoid"><%= doItem.runningNum %>  </div>
                                </td>
                                <td class="td-spacing"> 
                                    <div class="avoid"><%= doItem.workType %> </div>
                                </td>
                                <td class="td-spacing"> 
                                    <div class="avoid"><%- doItem.description.replace(/\n/g, '<br>') %> </div>
                                </td>
                                <td class="td-spacing"> </td>
                                <td class="td-spacing"> </td>
                                <td class="td-spacing"> 
                                    <div class="avoid"><%= doItem.qty %> </div>
                                </td>
                            </tr>

                            <% for (const pickItem of doItem.woPickingList) {%>
                                <tr style="border-bottom: 1px solid">
                                    <td class="td-spacing">
                                        <div class="avoid"><%=pickItem.runningNum%></div>
                                    </td>
                                    <td class="td-spacing"></td>
                                    <td class="td-spacing">
                                        <div class="avoid">(<%= pickItem.partNumber %>)<br/> <%- pickItem.productDesc.replace(/\n/g, '<br>') %> </div>
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= pickItem.selectedSkuPartNumber %></div> 
                                    </td>
                                    <td class="td-spacing"> 
                                        <div class="avoid"><%= pickItem.selectedSkuLocation %> </div>
                                    </td>
                                    <td class="td-spacing">
                                        <div class="avoid"><%=pickItem.workQty%></div>
                                    </td>
                                </tr>
                            <% } %>
                        <% } %>


                  <% } %>
                
                   
                  </table>

                </div>
            </div>
        </div>  
        <div class="spacing-20"></div>


        <!-- SECTION computer generated signature   -->
       
    <!-- </page> -->
    <!-- <br><br> -->
</body>

<script src="" async defer></script>

</html>